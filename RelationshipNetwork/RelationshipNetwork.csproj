    <None Remove="Delete.png" />
    <None Remove="Highlight.png" />
    <None Remove="Link.png" />
    <None Remove="OpenFile.png" />
    <None Remove="Redo.png" />
    <None Remove="Refresh.png" />
    <None Remove="SaveFile.png" />
    <None Remove="Sort.png" />
    <None Remove="Undo.png" />
