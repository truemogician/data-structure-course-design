    <TargetFramework>net5.0-windows</TargetFramework>
    <LangVersion>latest</LangVersion>
    <UseWPF>true</UseWPF>
    <AssemblyName>RelationshipNetwork</AssemblyName>
    <RootNamespace>RelationshipNetwork</RootNamespace>
    <ApplicationIcon>Network.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Add.png" />
    <None Remove="Assets\Add.png" />
    <None Remove="Assets\Delete.png" />
    <None Remove="Assets\Highlight.png" />
    <None Remove="Assets\Link.png" />
    <None Remove="Assets\Network.ico" />
    <None Remove="Assets\Network.png" />
    <None Remove="Assets\OpenFile.png" />
